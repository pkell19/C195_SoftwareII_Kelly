            <Button alignment="CENTER" contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#openApptByContact" prefWidth="200.0" style="-fx-background-color: #36454f;" text="Appointments by Contact" textFill="WHITE">
